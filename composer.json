{
    "name": "solunes/product",
    "description": "Paquete adicional de Productos para Solunes Digital.",
    "keywords": ["laravel", "solunes", "product"],
    "license": "MIT",
    "authors": [
        {
            "name": "Solunes Digital",
            "email": "info@solunes.com"
        }
    ],
    "require": {
        "solunes/master": "*",
        "solunes/business": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "classmap": [
            "src/database/migrations",
            "src/database/seeds",
            "src/app",
            "src/app/Helpers",
            "src/app/Middleware",
            "src/app/Listeners",
            "src/app/Routes",
            "src/app/Providers",
            "src/app/Controllers",
            "src/app/Traits",
            "src/app/Exceptions",
            "src/app/Console"
       ],
        "psr-4": {
            "Solunes\\Product\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    },
    "prefer-stable": true
}
